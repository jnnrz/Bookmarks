{
  "manifest_version": 2,
  "name": "Bookmarks",
  "description": "Save bookmarks for later",
  "version": "1.2.0",
  "icons": {
    "48": "images/bookmark48.png",
    "64": "images/bookmark64.png",
    "128": "images/bookmark128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/bookmark16.png",
      "24": "images/bookmark24.png",
      "32": "images/bookmark32.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "bookmarks",
    "contextMenus",
    "activeTab",
    "tabs",
    "storage",
    "chrome://favicon/"
  ]
}