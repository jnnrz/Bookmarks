{
  "manifest_version": 2,
  "name": "Bookmarks",
  "description": "Save bookmarks for later",
  "version": "0.1.0",
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "bookmarks",
    "contextMenus",
    "activeTab",
    "tabs",
    "storage"
  ]
}